{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "arrowFunctions": true,
    "binaryLiterals": true,
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": true,
    "generators": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "globalReturn": true,
    "jsx": true,
    "experimentalObjectRestSpread": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "amd": true
  },
  "globals": {
    "angular": true,
    "_saq": true,
    "FB": true,
    "d3": true,
    "google": true,
    "expect" : false,
    "describe" : false,
    "it": false,
  },
  "parser": "babel-eslint",
  "rules": {
    "no-var": 1,
    "no-with": 2,
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-template": 1,
    "object-shorthand": 0,
    "indent": [1, 2, { "SwitchCase": 1 }],
    "strict": 1,
    "no-underscore-dangle": 0,
    "no-unused-vars": [0, {"args": "none", "varsIgnorePattern": "tag|debug"}],
    "no-console": 0,
    "quotes": [ 2, "single" ]
  }
}

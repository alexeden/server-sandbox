{
  "name": "@app/dotstar-node-server",
  "main": "dist/index.js",
  "version": "0.0.0",
  "scripts": {
    "build": "rm -rf dist && tsc -p src",
    "start": "nodemon",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "dotstar-node": "file:../..",
    "express": "^4.17.1",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "@types/ramda": "^0.27.36",
    "@types/ws": "^7.4.0",
    "nodemon": "^2.0.7",
    "typescript": "^4.1.3"
  },
  "nodemonConfig": {
    "ext": "ts,json",
    "exec": "rm -rf dist && tsc -p src && node dist",
    "events": {},
    "watch": [
      "src/"
    ]
  }
}
